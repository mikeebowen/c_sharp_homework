<Window x:Class="HelloWorld.NavigatorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HelloWorld"
        mc:Ignorable="d"
        Title="NavigatorWindow" Height="450" Width="800">
    <StackPanel>
        <TextBlock>
            Enter an address (<Run Foreground="Red">Don't forget https://</Run>)
        </TextBlock>
        <TextBox x:Name="uxURLTextBox" />
        <Button x:Name="uxButton" Content="GO!" Click="uxButton_Click" />
    </StackPanel>
</Window>
